#include<stdio.h>
int main(){
    int n,count=0,count1=0;
    printf("enter number of elements:");
    scanf("%d\n",&n);

    int score[n];
    printf("enter %d number in the arrays:",n);
    for(int i=0; i<n; i++){
        scanf("%d \n",&score[i]);
    }
    
    int even_arr[n],odd_arr[n];
    for(int i=0; i<n; i++){
        if(score[i]%2==0 ){
            even_arr[count++]=score[i];
        }
        else{ odd_arr[count1++]=score[i];
        }
    }
    printf("score: ");
    for(int i=0; i<n; i++){
        printf("%d\n", score[i]);
    }

    printf("Even Score: ");
    for(int j=0; j<count; j++){
        printf("%d \n",even_arr[j]);
    }
        
    printf("Odd Score: ");
    for(int j=0; j<count1; j++){
        printf("%d \n",odd_arr[j]);
    }
    return 0;
}
