#include<stdio.h>
int main(){
    int n;
    int count=0;
    int isprime=1; 
    int num;
    printf("enter number of values in array: ");
    scanf("%d",&n);

    int arr[n];
    printf("enter %d values: ",n);
    for(int i=0; i<n; i++){
        scanf("%d\n",&arr[i]);
    }
    
    for(int i=0; i<n; i++){
        num=arr[i];
        if(arr[i]<=1){isprime=0;}
        else{
            for(int j=2; j*j<=num; j++){
                if(num%j==0){
                    isprime=0;
                    break;
                }
            }
        }
        if(isprime){count++;}
    }
    printf("prime numbers: %d",count);
    return 0;
}
