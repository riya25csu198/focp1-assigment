#include<stdio.h>
int main(){
    int n; 
    printf("enter number of values in array: ");
    scanf("%d\n",&n);

    int arr[n];
    printf("enter %d values: ",n);
    for(int i=0; i<n; i++){
        scanf("%d\n",&arr[i]);
    }

    if(n<=2){
        printf("peak Value not possible!");
    }else{
        printf("peak Value: \n");
        for(int i=1; i<n-1; i++){
            if(arr[i]>arr[i-1] && arr[i]>arr[i+1]){
                printf("%d ",arr[i]);
            }
        }
    }
    return 0;
}
