#include <stdio.h>
int main() {
    int n, m;
    printf("Enter total rows: ");
    scanf("%d", &n);

    printf("Enter total columns: ");
    scanf("%d", &m);

    int arr[n][m];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            printf("Enter element at row %d, column %d: ", i + 1, j + 1);
            scanf("%d", &arr[i][j]);
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (arr[i][j] == 99) {
                printf("99 occurs at row %d, column %d in the array\n", i + 1, j + 1);
            }
        }
    }
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);

    int arr2[n];
    for (int i = 0; i < n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr2[i]);
    }
    for (int i = 0; i < n; i++) {
        if (arr2[i] == 99) {
            printf("99 occurs at position %d in the array\n", i + 1);
        }
    }
    return 0;
}
