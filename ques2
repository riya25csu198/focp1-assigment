#include<stdio.h>
int main(){
    int number1, number2, i, hcf;
    
    printf("enter your first number: ");
    scanf("%d\n; &number1");
    
    printf("enter your second number: ");
    scanf("%d\n; &number2");
    
    for( i= 1; i<= (number1&&number2); ++i){
        if((number1&&number2) %i ==0 ){
            hcf=i;
        }   
     }
    printf("HCF of %d and %d is %d\n:", hcf,&number1,&number2);

    return 0;
}
